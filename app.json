{
  "expo": {
    "name": "ova",
    "slug": "ova",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "ova",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": ["expo-router", ["expo-splash-screen"]],
    "experiments": {
      "typedRoutes": true
    }
  }
}
